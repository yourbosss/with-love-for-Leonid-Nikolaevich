#include <iostream>
#include <cmath>
using namespace std;

int main() {
    float d = 19.1;
    float v = 0.009;
    double resist, R;
    
    double w[7] = {1, 2, 5, 10, 20, 50, 100};
    for (int i=0; i<7; ++i) {
        R = ((w[i] * d) / v); 
        
        if (R > 9.2 * pow(10, 5)) {
            resist = (0.021 / pow(d, (0.3)));
            cout << "R = " << R << "  resist = " << resist << endl;
        } else {
            resist = pow((1.5 * pow(10, (-6)) / d) + (1 / R), 0.3);
            cout << "R = " << R << "  resist = " << resist << endl;
         } 
    }
    return 0;
    }
